{
  "name": "@mcpjam/sdk",
  "version": "0.1.4",
  "type": "module",
  "description": "Official MCPJam SDK with reusable client utilities",
  "license": "Apache-2.0",
  "author": "MCPJam (https://mcpjam.com)",
  "homepage": "https://mcpjam.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/MCPJam/inspector.git",
    "directory": "sdk"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./mcp-client-manager": {
      "types": "./dist/mcp-client-manager/index.d.ts",
      "import": "./dist/mcp-client-manager/index.js",
      "require": "./dist/mcp-client-manager/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean --silent && tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "ai": "^5.0.76"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
